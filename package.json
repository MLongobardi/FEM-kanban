{
	"name": "svelte-template",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"myDev": "start /min gulp & vite dev --open & taskkill /FI \"windowtitle eq gulp\" ",
		"myDevBash": "gulp & pid1=\"$!\" ; vite dev --open ; kill $pid1",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"gulp": "^4.0.2",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"sass": "^1.53.0",
		"svelte": "^3.54.0",
		"svelte-preprocess": "^5.0.1",
		"vite": "^4.0.0"
	},
	"type": "module",

	"overrides": {
		"chokidar": "3.5.3",
		"glob-parent": "6.0.2"
	}
}
